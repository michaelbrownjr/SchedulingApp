<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="506.0" prefWidth="834.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.customerController">
    <padding>
        <Insets right="10.0"/>
    </padding>
    <TableView fx:id="customerTable" layoutX="14.0" layoutY="14.0" prefHeight="361.0" prefWidth="801.0">
        <columns>
            <TableColumn fx:id="customerIDColumn" prefWidth="72.0" text="CustomerID"/>
            <TableColumn fx:id="customerNameColumn" prefWidth="135.0" text="Customer name"/>
            <TableColumn fx:id="countryColumn" minWidth="3.0" prefWidth="67.0" text="Country"/>
            <TableColumn fx:id="divisionColumn" prefWidth="72.0" text="Division"/>
            <TableColumn fx:id="addressColumn" prefWidth="203.0" text="Address"/>
            <TableColumn fx:id="postalCodeColumn" prefWidth="93.0" text="Postal Code"/>
            <TableColumn fx:id="phoneNumberColumn" prefWidth="158.0" text="Phone Number"/>
        </columns>
    </TableView>
    <HBox alignment="CENTER" layoutX="14.0" layoutY="382.0" prefHeight="45.0" prefWidth="409.0" spacing="15.0"
          AnchorPane.bottomAnchor="79.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="411.0"
    >
        <children>
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#pressAddButton" prefHeight="25.0"
                    prefWidth="81.0" text="Add">
                <padding>
                    <Insets right="10.0"/>
                </padding>
            </Button>
            <Button fx:id="editButton" mnemonicParsing="false" onAction="#editButtonActivity" prefHeight="25.0"
                    prefWidth="81.0" text="Edit"/>
            <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteButtonActivity" prefHeight="25.0"
                    prefWidth="81.0" text="Delete"/>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonActivity" prefHeight="25.0"
                    prefWidth="81.0" text="Back"/>
        </children>
        <padding>
            <Insets left="10.0" right="10.0"/>
        </padding>
        <AnchorPane.topAnchor>382.0</AnchorPane.topAnchor>
        <opaqueInsets>
            <Insets bottom="10.0" right="10.0"/>
        </opaqueInsets>
    </HBox>
</AnchorPane>
